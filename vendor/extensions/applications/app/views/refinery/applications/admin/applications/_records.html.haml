- if searching?
  %h2= t('results_for', :scope => 'refinery.admin.search', :query => params[:search])

.pagination_container
  - if @applications.any?
    = render 'applications'
  - else
    %p
      - if searching?
        = t('no_results', :scope => 'refinery.admin.search')
      - else
        %strong= t('.no_items_yet')
